<h1>ジャンル一覧・追加</h1>
<%= form_with model: @genre, url: admin_genres_path do |f| %>
  <label>ジャンル名</label>
  <%= f.text_field :name %>
  <%= f.submit "新規登録" %>
<% end %>
<table>
  <thead>ジャンル</thead>
  <tbody>
    <% @genres.each do |genre| %>
    <tr>
      <td><%= genre.name %></td>
      <td><%= link_to "編集する", edit_admin_genre_path %></td>
    </tr>
    <% end %>
  </tbody>
</table>

    
