<h1>配送先変更</h1>
<%= form_with model: @address do |f| %>
　<table>
　  <tr>
　    <td><label>郵便番号(ハイフンなし)</label></td>
　    <td><%= f.text_field :postal_code %>  </td>
　  </tr>
　  <tr>
　    <td><label>住所</label></td>
　    <td><%= f.text_field :address %></td>
　  </tr>
　  <tr>
　    <td><label>宛名</label></td>
　    <td><%= f.text_field :name %></td>
　  </tr>
　  <td><%= f.submit "変更内容を保存" %></td>
　</table>
<% end %>