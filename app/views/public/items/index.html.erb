 <div class = "row">
  <thead>
   <th>ジャンル検索</th>
  </thead>
  <tbody>

   <td></td>
  </tbody>
  <h1>商品一覧</h1>

  <% @items.each do |item| %>
  <div><%= image_tag item.get_item_image(100,100) %></div>
   <%= link_to item_path(item.id),class: 'text-light' do %>
    <div><%= item.name %></div>
   <% end %>
   <div><%= item.price %></div>
  <% end %>
 </div>
